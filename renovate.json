{
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "match": "manager = 'dockerfile'",
      "additionalBranchPrefix": "docker-"
    }, {
      "match": "deprecationMessage = '*'",
      "labels": ["deprecated"]
    }, {
      "match": "branchName = '*-vulnerability'",
      "labels": ["security"]
    }
  ]
}
