{
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchQl": "manager = 'dockerfile'",
      "additionalBranchPrefix": "docker-"
    }, {
      "matchQl": "deprecationMessage = '*'",
      "labels": ["deprecated"]
    }, {
      "matchQl": "branchName = '/-vulnerability$/'",
      "labels": ["security"]
    }
  ]
}
